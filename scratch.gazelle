(define (range [
				((start step stop transform)
				 (var out [:])
				 (for ((var i start) 
					   (_< i stop) 
					   (set! i (_+ i step)))
					  (out.push 
					   (transform i)))
				 out)

				((start step stop)
				 (recur start step stop (lambda (v) v)))
				((start stop)
				 (recur start 1 stop (lambda (v) v)))
				((stop)
				 (recur 0 1 stop (lambda (v) v)))
				]))

(lambda ()
  (_newline-sequence 
   a b c))
